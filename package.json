{
    "name": "tab-anywhere",
    "publisher": "avasilev",
    "version": "1.2.4",
    "engines": {
        "vscode": "^1.31.0"
    },
    "displayName": "Tab Anywhere",
    "description": "Performs auto-indent or the current line on tab press, regardless of cursor position within the line",
    "categories": ["Linters", "Formatters"],
    "keywords": ["tab", "indent"],
    "main": "extension.js",
    "contributes": {
        "keybindings": [
            {
                "command": "tab",
                "key": "tab",
                "when": "editorTextFocus"
            }
        ]
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "devDependencies": {
        "vscode": "^1.1.36"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alxvasilev/tab-anywhere"
    }
}
